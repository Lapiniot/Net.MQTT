{
    "Kestrel": {
        "Endpoints": {
            "http": {
                "Url": "http://0.0.0.0:9000"
            }
        }
    },
    "MQTT": {
        "Endpoints": {
            "tcp.default": {
                "Url": "tcp://0.0.0.0:1883"
            },
            "tcp.ssl.default": {
                "Url": "tcp://0.0.0.0:1884",
                "Certificate": "Default",
                "SslProtocols": "None"
            }
        },
        "Certificates": {
            "Default": {
                "Path": "mqtt-server.pfx"
            }
        },
        "ConnectTimeout": 500
    },
    "WSListener": {
        "QueueCapacity": 100,
        "AcceptRules": {
            "/mqtt": [
                "mqtt",
                "mqttv3.1"
            ]
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Trace"
        }
    }
}