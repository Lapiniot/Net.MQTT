{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2AndHttp3"
    },
    "Endpoints": {
      "http": {
        "Url": "http://*:8001",
        "Protocols": "Http1"
      },
      "http/2": {
        "Url": "http://*:8003",
        "Protocols": "Http2"
      }
    }
  },
  "MQTT": {
    "Endpoints": {
      "tcp.default": {
        "Url": "tcp://[::]:1883"
      },
      "unix.socket": {
        "Url": "unix:///tmp/mqttd.sock"
      }
    },
    "ProtocolLevel": "All",
    "ConnectTimeout": 500,
    "MaxUnflushedBytes": 4096,
    "MaxInFlight": 32768,
    "MaxReceive": 32768,
    "MaxPacketSize": 4194304
  },
  "WSListener": {
    "QueueCapacity": 100,
    "AcceptProtocols": {
      "/mqtt": [
        "mqtt",
        "mqttv3.1"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionStrings": {
    "ApplicationDbContextConnection": "Data Source=./data/Mqtt.Server.db"
  },
  "MetricsCollectionSupport": true,
  "UseAdminWebUI": true,
  "AdminWebUI": {
    "AutoRefreshInterval": "00:00:03",
    "EventsThrottleInterval": "00:00:03"
  },
  "MetricsCollector": {
    "RecordInterval": "00:00:10"
  },
  "Metrics": {
    "EnabledMetrics": {
      "Net.Mqtt.Server": {
        "Default": false,
        "mqtt.server.bytes_rx": false,
        "mqtt.server.bytes_tx": false,
        "mqtt.server.packets_rx": false,
        "mqtt.server.packets_tx": false
      }
    }
  }
}