{
  "Kestrel": {
    "Endpoints": {
      "http": {
        "Url": "http://*:8001"
      }
    }
  },
  "MQTT": {
    "Endpoints": {
      "tcp.default": {
        "Url": "tcp://[::]:1883"
      },
      "unix.socket": {
        "Url": "unix:///tmp/mqttd.sock"
      }
    },
    "ProtocolLevel": "All",
    "ConnectTimeout": 500,
    "MaxUnflushedBytes": 4096
  },
  "WSListener": {
    "QueueCapacity": 100,
    "AcceptRules": {
      "/mqtt": [
        "mqtt",
        "mqttv3.1"
      ]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "ConnectionStrings": {
    "ApplicationDbContextConnection": "Data Source=Mqtt.Server.db"
  }
}