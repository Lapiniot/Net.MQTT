{
  "MQTT": {
    "Endpoints": {
      "tcp.default": "tcp://0.0.0.0:1883",
      "ws.default": "ws://localhost:8000/mqtt/"
    }
  },
  "WSListener": {
    "QueueCapacity": 100,
    "AcceptRules": {
      "/mqtt": ["mqtt", "mqttv3.1"]
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    }
  }
}