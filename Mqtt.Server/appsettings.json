{
    "Kestrel": {
        "Endpoints": {
            "http": {
                "Url": "http://0.0.0.0:9000"
            }
        }
    },
    "MQTT": {
        "Endpoints": {
            "tcp.default": "tcp://0.0.0.0:1883"
        }
    },
    "WSListener": {
        "QueueCapacity": 100,
        "AcceptRules": {
            "/mqtt": [
                "mqtt",
                "mqttv3.1"
            ]
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Trace"
        }
    }
}