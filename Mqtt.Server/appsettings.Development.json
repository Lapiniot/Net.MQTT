{
  "Kestrel": {
    "Endpoints": {
      "https": {
        "Url": "https://*:8002",
        "ClientCertificateMode": "AllowCertificate"
      },
      "mqtts-kestrel": {
        "Url": "https://*:8884",
        "Protocols": "Http1",
        "UseMqtt": true
      }
    },
    "Certificates": {
      "Default": {
        "Path": "mqtt-server.pfx"
      }
    }
  },
  "MQTT": {
    "Endpoints": {
      "mqtts": {
        "Address": "::",
        "Port": 8883,
        "Certificate": "Default",
        "ClientCertificateMode": "AllowCertificate"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "mqtt-server.pfx"
      }
    }
  },
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Net.Mqtt.Server": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  }
}