{
    "Kestrel": {
        "Endpoints": {
            "Https": {
                "Url": "https://*:8002",
                "Certificate": {
                    "Path": "mqtt-server.pfx"
                },
                "ClientCertificateMode": "AllowCertificate"
            }
        }
    },
    "MQTT": {
        "Endpoints": {
            "tcp.ssl.default": {
                "Url": "tcps://[::]:8883",
                "Certificate": "Default",
                "ClientCertificateMode": "AllowCertificate"
            }
        },
        "Certificates": {
            "Default": {
                "Path": "mqtt-server.pfx"
            }
        }
    }
}