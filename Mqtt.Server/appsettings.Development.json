{
  "Kestrel": {
    "Endpoints": {
      "https": {
        "Url": "https://*:8002",
        "ClientCertificateMode": "AllowCertificate"
      },
      "mqtts-kestrel": {
        "Url": "https://*:8884",
        "Protocols": "Http1",
        "UseMqtt": true
      }
    }
  },
  "MQTT": {
    "Endpoints": {
      "mqtts": {
        "Address": "::",
        "Port": 8883,
        "Certificate": {
          "Path": ""
        },
        "ClientCertificateMode": "AllowCertificate"
      }
    }
  },
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Net.Mqtt.Server": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  }
}