{
    "files.exclude": {
        ".gitmodules": true,
        "**/node_modules": true,
        "**/artifacts": true,
        "**/*.DotSettings": true,
        "**/bin": true,
        "**/obj": true,
        "**/publish": true,
        ".vs": true,
        "**/*.user": true,
        "*/**/.vscode": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "Mqtt.Benchmark/appsettings*.json"
            ],
            "url": ".vscode/schemas/mqtt.benchmark.settings.json"
        },
        {
            "fileMatch": [
                "Mqtt.Server/appsettings*.json"
            ],
            "url": ".vscode/schemas/mqtt.server.settings.json"
        },
        {
            "fileMatch": [
                "Mqtt.Server.AppHost/appsettings*.json"
            ],
            "url": ".vscode/schemas/db-provider.settings.json"
        }
    ],
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "files.associations": {
        "**/src/css/*.css": "tailwindcss"
    }
}