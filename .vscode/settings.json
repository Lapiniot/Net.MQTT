{
    "files.exclude": {
        ".gitignore": true,
        ".gitmodules": true,
        "**/*.DotSettings": true,
        "*.sln": false,
        "**/bin": true,
        "**/obj": true,
        ".vs": true,
        "**/*.user": true,
        "*/**/.vscode": true,
        "**/*.sln": true
    },
    "omnisharp.enableRoslynAnalyzers": true,
    "json.schemas": [
        {
            "fileMatch": [
                "Mqtt.Benchmark/appsettings*.json"
            ],
            "url": "./.vscode/schemas/mqtt.benchmark.settings.json"
        }
    ]
}